    <style>
        .bar {
            background: rgb(251, 249, 249);
            backdrop-filter: blur(25px);
            width: 250px;
            height: 100vh;
            position: fixed;
            top: 0;
            left: -250px;
            overflow-y: auto;
            transition: 0.2s ease;
            transition-property: left;
        }

            .bar::-webkit-scrollbar {
                width: 0px;
            }



            .bar.active {
                left: 0;
            }



            .bar .menu {
                width: 100%;
                margin-top: 20px;
            }

                .bar .menu .item {
                    position: relative;
                    cursor: pointer;
                }

                    .bar .menu .item a {
                        color: #696969;
                        font-size: 16px;
                        text-decoration: none;
                        display: block;
                        padding: 1px 30px;
                        line-height: 40px;
                    }

                        .bar .menu .item a:hover {
                            background: rgb(140, 215, 240);
                            transition: 0.1s ease;
                        }

                    .bar .menu .item i {
                        margin-right: 20px;
                    }

                    .bar .menu .item a .dropdown {
                        position: absolute;
                        right: 0;
                        margin: 15px;
                        transition: 0.2s ease;
                    }

                    .bar .menu .item .sub-menu {
                        display: none;
                    }

                        .bar .menu .item .sub-menu a {
                            padding-left: 35px;
                        }

        .rotate {
            transform: rotate(90deg);
        }

        .close-btn {
            position: absolute;
            color: rgb(140, 215, 240);
            font-size: 23px;
            right: 0px;
            margin: 18px;
            cursor: pointer;
        }

    </style>
